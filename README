  /usr/share/doc/gmysqlcc-0.2.6/AUTHORS
  /usr/share/doc/gmysqlcc-0.2.6/COPYING
  /usr/share/doc/gmysqlcc-0.2.6/ChangeLog
  /usr/share/doc/gmysqlcc-0.2.6/INSTALL
  /usr/share/doc/gmysqlcc-0.2.6/NEWS
  /usr/share/doc/gmysqlcc-0.2.6/README
  /usr/share/doc/gmysqlcc-0.2.6/TODO
  /usr/share/locale/es/LC_MESSAGES/gmysqlcc.mo
  /usr/share/locale/fr/LC_MESSAGES/gmysqlcc.mo
